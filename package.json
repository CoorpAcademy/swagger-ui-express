{
  "name": "@coorpacademy/swagger-ui-express",
  "version": "1.0.1",
  "description": "Express middleware to ship swagger ui",
  "main": "src/index.js",
  "scripts": {
    "eslint": "eslint src/ *.js *.json --ext .js,.json",
    "eslint:fix": "npm run eslint -- --fix",
    "ava": "ava",
    "nyc": "nyc npm run ava",
    "test": "npm run eslint && npm run nyc",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "author": "Coorpacademy Tech Team",
  "license": "MIT",
  "homepage": "https://github.com/Coorpacademy/swagger-ui-express",
  "repository": {
    "type": "git",
    "url": "https://github.com/Coorpacademy/swagger-ui-express.git"
  },
  "bugs": {
    "url": "https://github.com/Coorpacademy/swagger-ui-express/issues/new"
  },
  "dependencies": {
    "express": "^4.16.3",
    "serve-static": "^1.13.2",
    "swagger-ui-dist": "^3.12.1"
  },
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^8.1.0",
    "ava": "^0.25.0",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "eslint": "^4.18.2",
    "nyc": "^11.6.0",
    "supertest": "^3.0.0"
  },
  "ava": {
    "files": [
      "**/test/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "**/test/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "all": true
  }
}
