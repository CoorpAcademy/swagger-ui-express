{
  "name": "@coorpacademy/swagger-ui-express",
  "version": "1.0.1",
  "description": "Express middleware to ship swagger ui",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/ *.json --ext .js,.json",
    "lint:fix": "npm run eslint -- --fix",
    "ava": "ava",
    "nyc": "nyc npm run ava",
    "test": "npm run lint && npm run nyc",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "author": "Coorpacademy Tech Team",
  "license": "MIT",
  "homepage": "https://github.com/Coorpacademy/swagger-ui-express",
  "repository": {
    "type": "git",
    "url": "https://github.com/Coorpacademy/swagger-ui-express.git"
  },
  "bugs": {
    "url": "https://github.com/Coorpacademy/swagger-ui-express/issues/new"
  },
  "dependencies": {
    "express": "^4.16.3",
    "serve-static": "^1.13.2",
    "swagger-ui-dist": "^3.30.0"
  },
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^9.3.1",
    "ava": "^2.4.0",
    "babel-register": "^6.26.0",
    "codecov": "^3.7.1",
    "eslint": "^6.8.0",
    "nyc": "^15.1.0",
    "supertest": "^4.0.2"
  },
  "ava": {
    "files": [
      "**/test/**/*.js",
      "!**/fixtures/**/*"
    ]
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "**/test/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "all": true
  }
}
